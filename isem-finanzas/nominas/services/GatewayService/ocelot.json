{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/{path}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 4001
        }
      ],
      "UpstreamPathTemplate": "/route/{path}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete"
      ],
      "ReRouteIsCaseSensitive": true
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:4000"
  }
}